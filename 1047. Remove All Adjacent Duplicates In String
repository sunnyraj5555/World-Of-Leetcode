class Solution {
public:
    string removeDuplicates(string s) {
        stack<char> st;
        string ans ="";
        
        for(int i=0; i<s.size(); i++){
            if(st.empty()) st.push(s[i]);
            else if(st.top() == s[i]) st.pop();
            else st.push(s[i]);
        }
        
        // for(char c:s){
        //     if(st.empty()) st.push(c);
        //     else if(st.top() == c) st.pop();
        //     else st.push(c);
        // }
        
        while(!st.empty()){
            
            ans += st.top();
            st.pop();
        }
        
        reverse(ans.begin(), ans.end());
        
        return ans;
    }
};
