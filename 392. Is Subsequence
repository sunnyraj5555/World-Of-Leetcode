class Solution {
public:
    bool isSubsequence(string s, string t) {
        int left=0, right=0;
        int m = s.length();
        int n = t.length();
        
        while(left < m && right<n){
            
            if(s[left] == t[right])
                left++;
            right++;
            
        }
        if(left == m)
            return true;
        return false;
    }
};
